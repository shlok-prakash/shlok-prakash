#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
  //  int a=1;
    for(int i=0; i<n; i++){
        char  a='a';
        for (int j=0;j<n;j++){
            if(i+j>=n-1){
                cout<<a++<<" ";
            }
            else 
            cout<<" ";
        }
        cout<<endl;
    }
}
